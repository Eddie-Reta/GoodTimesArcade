  "bugs": {
    "url": "https://github.com/Eddie-Reta/GoodTimesArcade/issues"
  },
  "homepage": "https://github.com/Eddie-Reta/GoodTimesArcade#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^6.0.1",
    "onchange": "^7.0.0",
    "parallelshell": "^3.0.1"
  }
}
