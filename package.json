{
  "name": "goodtimesarcade",
  "version": "1.0.0",
  "description": "Website for an arcade and bar.",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o ./assets/css ./assets/css",
    "start": "npm run watch:all",
    "watch:scss": "onchange \"./assets/css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eddie-Reta/GoodTimesArcade.git"
  },
